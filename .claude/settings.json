{
  "model": "opus",
  "permissions": {
    "deny": [
      "Bash(rm -rf *)"
    ],
    "allow": [
      "Bash(git status)",
      "Bash(git status *)",
      "Bash(git diff)",
      "Bash(git diff *)",
      "Bash(git log)",
      "Bash(git log *)",
      "Bash(git show *)",
      "Bash(git branch)",
      "Bash(git branch -a)",
      "Bash(git branch -v)",
      "Bash(git stash list)",
      "Bash(ls)",
      "Bash(ls *)",
      "Bash(head *)",
      "Bash(wc *)",
      "Bash(mkdir *)",
      "Bash(mkdir -p *)"
    ]
  }
}
